                                                                <lightning:input aura:id="f" value="{!contactRelation.RelatedContact__r.JobHistory__r[0].Position__c}" label="{!v.describe.JobHistory__c.fields.Position__c.label}" name="cosignerSpouseJobHistoryPosition" disabled="{!v.params.readOnly}" maxlength="{!v.describe.JobHistory__c.fields.Position__c.length}" placeholder="{!$Label.c.Required}" required="true"/>
                                                                <lightning:input aura:id="f" value="{!contactRelation.RelatedContact__r.JobHistory__r[0].Salary__c}" label="{!v.describe.JobHistory__c.fields.Salary__c.label}" name="cosignerSpouseJobHistorySalary" disabled="{!v.params.readOnly}" formatter="currency" max="1e16" min="0" placeholder="{!$Label.c.Required}" required="true" step="any" type="number"/>
                                                            </div>
                                                        </aura:if>
                                                    </aura:iteration>
                                                    <div class="section">
                                                        <h2 class="slds-text-title_caps">{!v.label.s30}
                                                            <aura:if isTrue="{!!v.params.readOnly}">
                                                                <lightning:buttonIcon value="Child" alternativeText="{!$Label.c.Create}" iconName="utility:new" iconClass="newIcon" size="large" variant="bare" onclick="{!c.addCosignerContactRelation}"/>
                                                            </aura:if>
                                                        </h2>
                                                        <div class="relatedList">
                                                            <table>
                                                                <thead>
                                                                    <tr>
                                                                        <th><label class="slds-form-element__label"><abbr class="slds-required" title="{!$Label.c.Required}">*</abbr>{!v.label.l1}</label></th>
                                                                        <th><label class="slds-form-element__label"><abbr class="slds-required" title="{!$Label.c.Required}">*</abbr>{!v.describe.Contact.fields.LastName.label}</label></th>
                                                                        <th><label class="slds-form-element__label"><abbr class="slds-required" title="{!$Label.c.Required}">*</abbr>{!v.describe.Contact.fields.Birthdate.label}</label></th>
                                                                        <th><label class="slds-form-element__label"><abbr class="slds-required" title="{!$Label.c.Required}">*</abbr>{!v.label.l15}</label></th>
                                                                        <th><label class="slds-form-element__label"><abbr class="slds-required" title="{!$Label.c.Required}">*</abbr>{!v.label.l19}</label></th>
                                                                        <th><label class="slds-form-element__label"><abbr class="slds-required" title="{!$Label.c.Required}">*</abbr>{!v.label.l20}</label></th>
                                                                        <aura:if isTrue="{!!v.params.readOnly}">
                                                                            <th width="5%"><label class="slds-form-element__label">{!$Label.c.Action}</label></th>
                                                                        </aura:if>
                                                                    </tr>
                                                                </thead>
                                                                <tbody>
                                                                    <aura:iteration items="{!applicationRelation.Contact__r.ContactRelations__r}" var="contactRelation" indexVar="index">
                                                                        <aura:if isTrue="{!contactRelation.Relationship__c == 'Child'}">
                                                                            <tr>
                                                                                <td><lightning:input aura:id="f" value="{!contactRelation.RelatedContact__r.FirstName}" label=" " name="cosignerChildFirstName" disabled="{!v.params.readOnly}" maxlength="{!v.describe.Contact.fields.FirstName.length}" placeholder="{!$Label.c.Required}" required="true"/></td>
                                                                                <td><lightning:input aura:id="f" value="{!contactRelation.RelatedContact__r.LastName}" label=" " name="cosignerChildLastName" disabled="{!v.params.readOnly}" maxlength="{!v.describe.Contact.fields.LastName.length}" placeholder="{!$Label.c.Required}" required="true"/></td>
                                                                                <td><lightning:input aura:id="f" value="{!contactRelation.RelatedContact__r.Birthdate}" label=" " name="cosignerChildBirthdate" disabled="{!v.params.readOnly}" placeholder="{!$Label.c.Required}" required="true" type="date" onchange="{!c.onPastDateChange}"/></td>
                                                                                <td><lightning:input aura:id="f" value="{!contactRelation.RelatedContact__r.Title}" label=" " name="cosignerChildTitle" disabled="{!v.params.readOnly}" maxlength="{!v.describe.Contact.fields.Title.length}" placeholder="{!$Label.c.Required}" required="true"/></td>
                                                                                <td><lightning:input aura:id="f" value="{!contactRelation.RelatedContact__r.ActualSchoolCompany__c}" label=" " name="cosignerChildActualSchoolCompany" disabled="{!v.params.readOnly}" maxlength="{!v.describe.Contact.fields.ActualSchoolCompany__c.length}" placeholder="{!$Label.c.Required}" required="true"/></td>
                                                                                <td><lightning:input aura:id="f" value="{!contactRelation.RelatedContact__r.Description}" label=" " name="cosignerChildDescription" disabled="{!v.params.readOnly}" maxlength="{!v.describe.Contact.fields.Description.length}" placeholder="{!$Label.c.Required}" required="true" step="any" type="number"/></td>
                                                                                <aura:if isTrue="{!!v.params.readOnly}">
                                                                                    <td class="slds-text-align_center"><lightning:buttonIcon value="{!index}" alternativeText="{!$Label.c.Delete}" iconName="utility:delete" iconClass="deleteIcon" variant="bare" onclick="{!c.removeCosignerContactRelation}"/></td>
                                                                                </aura:if>
                                                                            </tr>
                                                                        </aura:if>
                                                                    </aura:iteration>
                                                                </tbody>
                                                            </table>
                                                        </div>
                                                    </div>
                                                    <aura:iteration items="{!applicationRelation.Contact__r.ContactRelations__r}" var="contactRelation" indexVar="index">
                                                        <aura:if isTrue="{!contactRelation.Relationship__c == 'Personal Reference'}">
                                                            <div class="section">
                                                                <h2 class="slds-text-title_caps">{!v.label.s31+' '+index}</h2>
                                                                <lightning:input aura:id="f" value="{!contactRelation.RelatedContact__r.FirstName}" label="{!v.label.l1}" name="cosignerPersonalReferenceFirstName" disabled="{!v.params.readOnly}" maxlength="{!v.describe.Contact.fields.FirstName.length}" placeholder="{!$Label.c.Required}" required="true"/>
                                                                <lightning:input aura:id="f" value="{!contactRelation.RelatedContact__r.LastName}" label="{!v.describe.Contact.fields.LastName.label}" name="cosignerPersonalReferenceLastName" disabled="{!v.params.readOnly}" maxlength="{!v.describe.Contact.fields.LastName.length}" placeholder="{!$Label.c.Required}" required="true"/>
                                                                <lightning:input aura:id="f" value="{!contactRelation.RelatedContact__r.Email}" label="{!v.label.l2}" name="cosignerPersonalReferenceEmail" disabled="{!v.params.readOnly}" maxlength="{!v.describe.Contact.fields.Email.length}" placeholder="{!$Label.c.Required}" required="true" type="email"/>
                                                                <lightning:input aura:id="f" value="{!contactRelation.RelatedContact__r.MobilePhone}" label="{!v.label.l3}" name="cosignerPersonalReferenceMobilePhone" disabled="{!v.params.readOnly}" maxlength="{!v.describe.Contact.fields.MobilePhone.length}" messageWhenPatternMismatch="{!v.label.m1}" pattern="[0-9]{10}" placeholder="5510008888" required="true" type="tel"/>
                                                                <lightning:input aura:id="f" value="{!contactRelation.RelatedContact__r.MailingStreet}" label="{!v.label.l21}" name="cosignerPersonalReferenceMailingStreet" disabled="{!v.params.readOnly}" maxlength="{!v.describe.Contact.fields.MailingStreet.length}" placeholder="{!$Label.c.Required}" required="true"/>
                                                                <lightning:input aura:id="f" value="{!contactRelation.RelatedContact__r.MailingNumber__c}" label="{!v.label.l22}" name="cosignerPersonalReferenceMailingNumber" disabled="{!v.params.readOnly}" maxlength="{!v.describe.Contact.fields.MailingNumber__c.length}" placeholder="{!$Label.c.Required}" required="true"/>
                                                                <lightning:input aura:id="f" value="{!contactRelation.RelatedContact__r.MailingApartmentNumber__c}" label="{!v.label.l23}" name="cosignerPersonalReferenceMailingApartmentNumber" disabled="{!v.params.readOnly}" maxlength="{!v.describe.Contact.fields.MailingApartmentNumber__c.length}" placeholder="{!$Label.c.Optional}"/>
                                                                <lightning:input aura:id="f" value="{!contactRelation.RelatedContact__r.MailingDistrict__c}" label="{!v.label.l24}" name="cosignerPersonalReferenceMailingDistrict" disabled="{!v.params.readOnly}" maxlength="{!v.describe.Contact.fields.MailingDistrict__c.length}" placeholder="{!$Label.c.Required}" required="true"/>
                                                                <lightning:input aura:id="f" value="{!contactRelation.RelatedContact__r.MailingCity}" label="{!v.label.l25}" name="cosignerPersonalReferenceMailingCity" disabled="{!v.params.readOnly}" maxlength="{!v.describe.Contact.fields.MailingCity.length}" placeholder="{!$Label.c.Required}" required="true"/>
                                                                <lightning:input aura:id="f" value="{!contactRelation.RelatedContact__r.MailingState}" label="{!v.label.l26}" name="cosignerPersonalReferenceMailingState" disabled="{!v.params.readOnly}" maxlength="{!v.describe.Contact.fields.MailingState.length}" placeholder="{!$Label.c.Required}" required="true"/>
                                                                <lightning:select aura:id="f" value="{!contactRelation.RelatedContact__r.MailingCountry}" label="{!v.label.l27}" name="cosignerPersonalReferenceMailingCountry" disabled="{!v.params.readOnly}" required="true">
                                                                    <option value="" text="{!'-- '+$Label.c.Select+' --'}"/>
                                                                    <aura:iteration items="{!v.describe.Contact.fields.CarrierCountry__c.picklistValues}" var="picklistEntry">
                                                                        <option value="{!picklistEntry.value}" text="{!picklistEntry.label}"/>
                                                                    </aura:iteration>
                                                                </lightning:select>
                                                                <lightning:input aura:id="f" value="{!contactRelation.RelatedContact__r.MailingPostalCode}" label="{!v.label.l28}" name="cosignerPersonalReferenceMailingPostalCode" disabled="{!v.params.readOnly}" maxlength="{!v.describe.Contact.fields.MailingPostalCode.length}" pattern="[0-9]{5}" placeholder="{!$Label.c.Required}" required="true"/>
                                                                <lightning:input aura:id="f" value="{!contactRelation.RelatedContact__r.Description}" label="{!v.describe.ContactRelation__c.fields.Relationship__c.label}" name="cosignerPersonalReferenceDescription" disabled="{!v.params.readOnly}" maxlength="{!v.describe.Contact.fields.Description.length}" placeholder="{!$Label.c.Required}" required="true"/>
                                                            </div>
                                                        </aura:if>
                                                    </aura:iteration>
                                                    <aura:iteration items="['Asset', 'Liability']" var="concept">
                                                        <div class="section balance">
                                                            <h2 class="slds-text-title_caps">{!concept == 'Asset' ? v.label.s36 : v.label.s37}</h2>
                                                            <div class="slds-form-element lightningInput">
                                                                <label class="slds-form-element__label slds-no-flex">{!v.describe.BalanceDetail__c.fields.Concept__c.label}</label>
                                                                <label class="slds-form-element__label slds-no-flex">{!v.describe.BalanceDetail__c.fields.Value__c.label} (MXN)</label>
                                                            </div>
                                                            <aura:iteration items="{!applicationRelation.Contact__r.Balances__r[0].BalanceDetails__r}" var="balanceDetail" indexVar="index">
                                                                <aura:if isTrue="{!or(and(concept == 'Asset', and(balanceDetail.Type__c == 'Asset', index &lt; 5)), and(concept == 'Liability', balanceDetail.Type__c == 'Liability'))}">
                                                                    <lightning:input aura:id="f" value="{!balanceDetail.Value__c}" label="{!balanceDetail.Concept__c}" name="cosignerBalanceDetail" disabled="{!v.params.readOnly}" formatter="currency" max="1e16" min="0" placeholder="{!$Label.c.Required}" required="true" step="any" type="number" onchange="{!c.setTotals}"/>
                                                                </aura:if>
                                                            </aura:iteration>
                                                            <aura:if isTrue="{!concept == 'Asset'}">
                                                                <div class="slds-form-element lightningInput">
                                                                    <label class="slds-form-element__label slds-no-flex">{!v.label.l35}</label>
                                                                    <aura:if isTrue="{!!v.params.readOnly}">
                                                                        <lightning:buttonIcon value="{!index}" alternativeText="{!$Label.c.Create}" iconName="utility:new" iconClass="newIcon" size="large" variant="bare" onclick="{!c.addCosignerProperty}"/>
                                                                    </aura:if>
                                                                </div>
                                                                <aura:iteration items="{!applicationRelation.Contact__r.Balances__r[0].BalanceDetails__r}" var="balanceDetail" indexVar="index">
                                                                    <aura:if isTrue="{!and(balanceDetail.Type__c == 'Asset', index > 4)}">
                                                                        <aura:if isTrue="{!!v.params.readOnly}">
                                                                            <lightning:buttonIcon value="{!index}" alternativeText="{!$Label.c.Delete}" iconName="utility:delete" iconClass="deleteIcon" variant="bare" onclick="{!c.removeCosignerProperty}"/>
                                                                        </aura:if>
                                                                        <lightning:input aura:id="f" value="{!balanceDetail.Concept__c}" label="{!v.label.l36}" name="cosignerPropertyConcept" disabled="{!v.params.readOnly}" maxlength="{!v.describe.BalanceDetail__c.fields.Concept__c.length}" placeholder="{!$Label.c.Required}" required="true"/>
                                                                        <lightning:input aura:id="f" value="{!balanceDetail.Location__c}" label="{!v.describe.BalanceDetail__c.fields.Location__c.label}" name="cosignerPropertyLocation" disabled="{!v.params.readOnly}" maxlength="{!v.describe.BalanceDetail__c.fields.Location__c.length}" placeholder="{!$Label.c.Required}" required="true"/>
                                                                        <lightning:input aura:id="f" value="{!balanceDetail.Value__c}" label="{!v.describe.BalanceDetail__c.fields.Value__c.label}" name="cosignerPropertyValue" disabled="{!v.params.readOnly}" formatter="currency" max="1e16" min="0" placeholder="{!$Label.c.Required}" required="true" step="any" type="number" onchange="{!c.setTotals}"/>
                                                                    </aura:if>
                                                                </aura:iteration>
                                                            </aura:if>
                                                            <lightning:input aura:id="f" value="{!concept == 'Asset' ? v.label.Asset : v.label.Liability}" label="{!$Label.c.Total}" name="cosignerTotalAsset" disabled="true" formatter="currency" type="number"/>
                                                        </div>
                                                    </aura:iteration>
                                                    <div class="acceptPrivacyStatement">
                                                        <lightning:input checked="{!applicationRelation.Contact__r.PrivacyStatementCheck__c}" label=" " name="cosignerPrivacyStatementCheck" disabled="{!v.params.readOnly}" type="checkbox"/>
                                                        <label class="slds-form-element__label">{!v.label.p1}<a onclick="{!c.togglePrivacyStatement}">{!v.label.p2}</a>.</label>
                                                    </div>
                                                    <aura:if isTrue="{!!v.params.readOnly}">
                                                        <div class="slds-text-align_center">
                                                            <lightning:button label="{!$Label.c.Save}" onclick="{!c.save}"/>
                                                            <lightning:button label="{!$Label.c.Submit}" disabled="{!!applicationRelation.Contact__r.PrivacyStatementCheck__c}" variant="brand" onclick="{!c.submitCosigner}"/>
                                                        </div>
                                                    </aura:if>
                                                    <aura:set attribute="else">
                                                        <aura:if isTrue="{!!v.params.isCosignerSubmitted}">
                                                            <p class="fullSize"><lightning:formattedRichText value="{!format(v.label.te1, $Label.c.Required)}"/></p>
                                                            <div class="section fullSize">
                                                                <h2 class="slds-text-title_caps">{!v.label.s33}</h2>
                                                                <c:ApplicationDocuments application="{!v.application}" contactId="{!v.contactId}" describe="{!v.describe}" params="{!v.params}" role="{!applicationRelation.Type__c}"/>
                                                            </div>
                                                            <aura:if isTrue="{!or(v.application.Status__c == 'APPROVED - APPROVED', v.application.Status__c == 'APPROVED - DOCUMENT CHECK')}">
                                                                <div class="slds-text-align_center">
                                                                    <lightning:button aura:id="submit" label="{!$Label.c.Submit}" value="true" variant="brand" onclick="{!c.submitCosigner}"/>
                                                                </div>
                                                            </aura:if>
                                                            <aura:set attribute="else">
                                                                <div class="fullSize slds-text-align_center">
                                                                    <h1 class="slds-text-heading_large slds-m-bottom_medium">{!v.label.tl1}</h1>
                                                                    <p class="slds-text-heading_medium"><lightning:formattedText value="{!v.label.tl2}" linkify="true"/></p>
                                                                </div>
                                                            </aura:set>
                                                        </aura:if>
                                                    </aura:set>
                                                </aura:if>
                                            </aura:set>
                                        </aura:if>
                                    </aura:if>
                                </aura:if>
                            </aura:iteration>
                        </aura:set>

                    </aura:if>

                </aura:set>

            </aura:if>
        </section>

        <c:AppFooter params="{!v.params}"/>

    </aura:if>


    <!--SI EXISTE REGISTRO EN LENDING PRODUCT-->
    <!-- <aura:if isTrue="{!v.application.LendingProduct__c}"> -->

        <!--AMORTIZACION-->
        <section aura:id="amortization" role="dialog" aria-modal="true" class="slds-fade-in-open slds-modal slds-hide">
            <div class="slds-backdrop slds-backdrop_open"></div>
            <div id="amortization" class="slds-modal__container">
                <header class="slds-modal__header">
                    <lightning:buttonIcon alternativeText="{!$Label.c.Close}" iconName="utility:close" class="slds-button_icon-inverse slds-modal__close" size="large" variant="bare" onclick="{!c.toggleAmortization}"/>
                    <h2 class="slds-text-heading_medium">{!v.describe.loan__Repayment_Schedule__c.labelPlural}
                        <lightning:helptext content="{!v.label.h11}"/>
                    </h2>
                </header>
                <div class="slds-modal__content slds-p-around_medium">
                    <iframe src="{!$Label.c.MagentrixPortalUrl+'/aspx/TEM_Calculator?tax=0.16&amp;graceOffset=2&amp;interest='+v.application.InterestRate__c+'&amp;term='+v.application.Term__c+(v.application.OriginalLoanAmount__c ? '&amp;loanAmount='+v.application.OriginalLoanAmount__c : '')+(v.application.DurationProgram__c ? '&amp;graceTerm='+v.application.DurationProgram__c : '')+(v.application.NumberOfDisbursements__c ? '&amp;disbursements='+v.application.NumberOfDisbursements__c : '')+(v.application.ExpectedStartDate__c ? '&amp;dateInput='+v.application.ExpectedStartDate__c : '')+(v.application.InsuranceFee__c ? '&amp;insuranceFee='+v.application.InsuranceFee__c*(1-0.16) : '')}" width="100%" height="600px"/>
                </div>
                <footer class="slds-modal__footer">
                    <lightning:button label="{!$Label.c.Close}" onclick="{!c.toggleAmortization}"/>
                </footer>
            </div>
        </section>

        <!--PRIVACIDAD-->
        <section aura:id="privacyStatement" role="dialog" aria-modal="true" class="slds-fade-in-open slds-modal slds-hide">
                <div class="slds-backdrop slds-backdrop_open"></div>
                <div id="privacyStatement" class="slds-modal__container">
                    <header class="slds-modal__header">
                        <lightning:buttonIcon alternativeText="{!$Label.c.Close}" iconName="utility:close" class="slds-button_icon-inverse slds-modal__close" size="large" variant="bare" onclick="{!c.togglePrivacyStatement}"/>
                        <h2 class="slds-text-heading_medium">{!v.label.p3}</h2>
                    </header>
                    <div class="slds-modal__content slds-p-around_medium">
                        <lightning:formattedRichText value="{!v.label.p4}"/>
                    </div>
                    <footer class="slds-modal__footer">
                        <lightning:button label="{!$Label.c.Close}" onclick="{!c.togglePrivacyStatement}"/>
                    </footer>
                </div>
            </section>


    <!-- </aura:if> -->
    
    
</aura:component>